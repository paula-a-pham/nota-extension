{
  "manifest_version": 3,
  "name": "Nota",
  "version": "1.0",
  "description": "Nota",
  "icons": {
    "16": "assets/icons/nota-16.png",
    "32": "assets/icons/nota-32.png",
    "48": "assets/icons/nota-48.png",
    "64": "assets/icons/nota-64.png",
    "128": "assets/icons/nota-128.png"
  },
  "action": {
    "default_title": "Nota",
    "default_icon": {
      "16": "assets/icons/nota-16.png",
      "32": "assets/icons/nota-32.png",
      "48": "assets/icons/nota-48.png",
      "64": "assets/icons/nota-64.png",
      "128": "assets/icons/nota-128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel"],
  "host_permissions": ["<all_urls>"]
}
